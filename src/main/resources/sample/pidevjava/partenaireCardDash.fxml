            <Label fx:id="nom" prefHeight="27.0" prefWidth="85.0" styleClass="label" text="Label" />
            <Label fx:id="prenom" prefHeight="27.0" prefWidth="80.0" styleClass="label" text="Label" />
            <Label fx:id="tel" prefHeight="27.0" prefWidth="127.0" styleClass="label" text="Label" />
