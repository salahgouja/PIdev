<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="45.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.pidevjava.controller.EventCardController">

    <children>
        <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="640.0" styleClass="card" stylesheets="@style/eventCssFile.css">
            <children>
                <Label fx:id="paticipationUser" styleClass="label" text="Label" />
                <Label fx:id="paticipationEvnt" styleClass="label" text="Label" />
                <Label fx:id="paticipationDate" styleClass="label" text="Label" />
            <Pane prefHeight="44.0" prefWidth="219.0" />
            <Button mnemonicParsing="false" onAction="#participationaccepter" text="accepter" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#participationRefuser" text="Refuser">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin></Button>
            </children>
            <padding>
            </padding>
        </HBox>
    </children>
    <styleClass>
        <String fx:value="card" />
        <String fx:value="shadow" />
    </styleClass>

</AnchorPane>
